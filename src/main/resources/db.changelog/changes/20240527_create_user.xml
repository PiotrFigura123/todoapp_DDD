<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="1" author="pfigura">
        <createTable tableName="users">
            <column name="id" autoIncrement="true" type="BIGINT"></column>
            <column name="firstName" type="VARCHAR(30)"></column>
            <column name="secondName" type="VARCHAR(30)"></column>
            <column name="password" type="VARCHAR(30)"></column>
            <column name="email" type="VARCHAR(30)"></column>
            <column name="role" type="BIGINT"></column>

        </createTable>
    </changeSet>
</databaseChangeLog>